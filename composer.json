{
    "name": "oxygen/mod-pages",
    "description": "Pages Module for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "oxygen/auth": "dev-laravel10",
        "oxygen/core": "dev-laravel10",
        "oxygen/crud": "dev-laravel10",
        "oxygen/data": "dev-laravel10",
        "oxygen/preferences": "dev-laravel10",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OxygenModule\\Pages\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OxygenModule\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/oxygen-cms/laraguard-doctrine"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpspec/phpspec": "^6.1"
    },
    "scripts": {
        "test": "vendor/bin/phpspec run && vendor/bin/phpstan analyze src --level 1 --memory-limit 256M"
    }
}
